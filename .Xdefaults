!-- Xft settings -- !
#Xft.dpi:        96
#Xft.antialias:  false
#Xft.rgba:       rgb
#Xft.hinting:    true
#Xft.hintstyle:  hintslight

! -- URxvt base settings -- !
URxvt.perl-ext-common: default,tabbedex,matcher,url-select,vtwheel
URxvt.termName:        xterm-256color
URxvt.background:      Black
URxvt.foreground:      Green
URxvt.scrollBar:       false

! -- URxvt Colors -- !
URxvt.color0:  #000000
URxvt.color1:  #cd0000
URxvt.color2:  #00cd00
URxvt.color3:  #cdcd00
URxvt.color4:  #1e90ff
URxvt.color5:  #cd00cd
URxvt.color6:  #00cdcd
URxvt.color7:  #e5e5e5
URxvt.color8:  #4c4c4c
URxvt.color9:  #ff0000
URxvt.color10: #00ff00
URxvt.color11: #ffff00
URxvt.color12: #4682b4
URxvt.color13: #ff00ff
URxvt.color14: #00ffff
URxvt.color15: #ffffff

! -- URxvt tabbed settings -- !
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0

! -- URxvt URL settings -- !
URxvt.url-launcher:         google-chrome 
URxvt.matcher.button:       1
URxvt.colorUL:              #4682B4
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.url-select.launcher:  google-chrome 
URxvt.url-select.underline: true

! -- URxvt Font settings -- !
#URxvt.font:           xft:Droid Sans Mono Slashed:pixelsize=11:Regular
#URxvt.boldFont:       xft:Droid Sans Mono Slashed:pixelsize=11:Bold
#URxvt.talicFont:      xft:Droid Sans Mono Slashed:pixelsize=11:Italic
#URxvt.bolditalicFont: xft:Droid Sans Mono Slashed:pixelsize=11:Bold:Italic
#URxvt.font:           xft:Ubuntu Mono:pixelsize=14,style=regular
URxvt.font:            8x13
